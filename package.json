    "core-js": "^3.6.4",
    "driver.js": "^0.9.8",
    "dropzone": "^5.7.0",
    "echarts": "^4.6.0",
    "element-ui": "^2.13.0",
    "file-saver": "^2.0.2",
    "fuse.js": "^3.4.6",
    "js-cookie": "^2.2.1",
    "jsonlint": "1.6.3",
    "jszip": "^3.2.2",
    "lodash": "^4.17.15",
    "normalize.css": "^8.0.1",
